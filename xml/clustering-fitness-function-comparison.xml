        <problem id="iris.unscaled.kharmonic.means" class="problem.ClusteringProblem" domain="R(4.2,8.0),R(1.9,4.5),R(0.9,6.9),R(0.0,2.6)">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.KHarmonicMeansFunction" />
