<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE simulator [
<!ATTLIST algorithm id ID #IMPLIED>
<!ATTLIST dataSetBuilder id ID #IMPLIED>
<!ATTLIST problem id ID #IMPLIED>
<!ATTLIST measurements id ID #IMPLIED>
]>

<simulator>
    <algorithms>
        <algorithm id="pso" class="pso.PSO">
            <addStoppingCondition class="stoppingcondition.MaximumIterations" maximumIterations="1000" />

            <iterationStrategy class="pso.iterationstrategies.SynchronousIterationStrategy">
                <boundaryConstraint class="problem.boundaryconstraint.RandomBoundaryConstraint" />
            </iterationStrategy>
        </algorithm>
    </algorithms>

    <dataSetBuilder id="artificial" class="problem.dataset.StaticDataSetBuilder" identifier="artificial">
        <addDataSet class="problem.dataset.LocalDataSet" identifier="../../datasets/generated/artificial2/artificial.txt" delimiter="\s+" beginIndex="0" endIndex="1" classIndex="2" />
    </dataSetBuilder>

    <dataSetBuilder id="iris.unscaled" class="problem.dataset.StaticDataSetBuilder" identifier="iris.unscaled">
        <addDataSet class="problem.dataset.LocalDataSet" identifier="../../datasets/uci/iris/iris.data" delimiter="," beginIndex="0" endIndex="3" classIndex="4" />
    </dataSetBuilder>

    <dataSetBuilder id="iris.scaled" class="problem.dataset.StaticDataSetBuilder" identifier="iris.scaled">
        <addDataSet class="problem.dataset.LocalDataSet" identifier="../../datasets/uci/iris/iris.scaled.csv" delimiter="," beginIndex="0" endIndex="3" classIndex="4" />
    </dataSetBuilder>

    <dataSetBuilder id="glass.unscaled" class="problem.dataset.StaticDataSetBuilder" identifier="glass.unscaled">
        <addDataSet class="problem.dataset.LocalDataSet" identifier="../../datasets/uci/glass/glass.data" delimiter="," beginIndex="1" endIndex="9" classIndex="10" />
    </dataSetBuilder>

    <dataSetBuilder id="glass.scaled" class="problem.dataset.StaticDataSetBuilder" identifier="glass.scaled">
        <addDataSet class="problem.dataset.LocalDataSet" identifier="../../datasets/uci/glass/glass.scaled.csv" delimiter="," beginIndex="1" endIndex="9" classIndex="10" />
    </dataSetBuilder>

    <problems>
        <problem id="artificial.kharmonic.means" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.KHarmonicMeansFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.non.parametric" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.NonParametricClusteringFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.parametric" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.ParametricClusteringFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.parametric.quantisation.error" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.ParametricWithQuantisationErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.quantisation.error" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.QuantisationErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.davies.bouldin.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.validityindices.DaviesBouldinIndex" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.dunn.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMaximisationProblem">
                <function class="functions.clustering.validityindices.DunnIndex" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.dunn.index33" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMaximisationProblem">
                <function class="functions.clustering.validityindices.DunnIndex33" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.dunn.index53" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMaximisationProblem">
                <function class="functions.clustering.validityindices.DunnIndex53" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.halkidi.vazirgiannis.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.validityindices.HalkidiVazirgiannisIndex" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.maulik.bandyopadhyay.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
