        <problem id="artificial.turi.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.validityindices.TuriIndex" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.veenman.reinders.backer.index" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.validityindices.VeenmanReindersBackerIndex" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>
        <problem id="artificial.total" class="problem.ClusteringProblem" domain="R(-1.0,1.0)^2">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.TotalErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="artificial" />
        </problem>

        <problem id="iris.unscaled.total" class="problem.ClusteringProblem" domain="R(4.2,8.0),R(1.9,4.5),R(0.9,6.9),R(0.0,2.6)">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.TotalErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="iris.unscaled" />
        </problem>

        <problem id="iris.scaled.total" class="problem.ClusteringProblem" domain="R(0.0,1.0)^4">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.TotalErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="iris.scaled" />
