        </problem>

        <problem id="glass.unscaled.total" class="problem.ClusteringProblem" domain="R(1.5,1.6),R(10.7,17.4),R(0.0,4.5),R(0.2,3.6),R(69.8,75.5),R(0.0,6.3),R(5.4,16.2),R(0.0,3.2),R(0.0,0.6)">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.TotalErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="glass.unscaled" />
        </problem>

        <problem id="glass.scaled.total" class="problem.ClusteringProblem" domain="R(0.0,1.0)^9">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.TotalErrorFunction" />
            </innerProblem>
            <dataSetBuilder idref="glass.scaled" />
        </problem>

        <problem id="glass.unscaled.kharmonic.means" class="problem.ClusteringProblem" domain="R(1.5,1.6),R(10.7,17.4),R(0.0,4.5),R(0.2,3.6),R(69.8,75.5),R(0.0,6.3),R(5.4,16.2),R(0.0,3.2),R(0.0,0.6)">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.KHarmonicMeansFunction" />
            </innerProblem>
            <dataSetBuilder idref="glass.unscaled" />
        </problem>

        <problem id="glass.scaled.kharmonic.means" class="problem.ClusteringProblem" domain="R(0.0,1.0)^9">
            <innerProblem class="problem.FunctionMinimisationProblem">
                <function class="functions.clustering.KHarmonicMeansFunction" />
            </innerProblem>
            <dataSetBuilder idref="glass.scaled" />
        </problem>
    </problems>

    <measurements id="measurements" class="simulator.MeasurementSuite" resolution="100">
        <addMeasurement class="measurement.single.Fitness" />
        <addMeasurement class="measurement.single.clustering.NumberOfClustersFormed" />
        <addMeasurement class="measurement.single.clustering.PlotHighDimensionalClustersAndCentroids" />
<!--
        <addMeasurement class="measurement.single.clustering.PlotClustersAndCentroids" />
-->
    </measurements>

    <simulations>
        <simulation samples="1">
            <algorithm idref="pso" />
            <problem idref="artificial.kharmonic.means" numberOfClusters="4" />
            <measurements idref="measurements" />
            <output format="TXT" file="measurements/pso.kharmonic.means.artificial.4.txt" />
        </simulation>

        <simulation samples="1">
